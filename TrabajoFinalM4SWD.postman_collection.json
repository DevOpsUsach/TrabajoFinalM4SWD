		{
			"name": "CasoSaldoAhorradoSuperiorA150UF",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Diez x Ciento del saldo ahorrado superior a 150 UF deberá devolver el tope máximo de retiro\", () => {\r",
							"  const responseJson = pm.response.json();\r",
							"  pm.expect(responseJson.ahorro).to.eql(50000000);\r",
							"  pm.expect(responseJson.dxc).to.eql(responseJson.maximoRetiro);\r",
							"});\r",
							"\r",
							"pm.test(\"Codigo de estado 200\", () => {\r",
							"  pm.response.to.have.status(200);\r",
							"});\r",
							"\r",
							"pm.test(\"Tiempo de respuesta menor a 200ms\", function () {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(200);\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
			],
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "http://104.248.59.191:8080/rest/msdxc/dxc?ahorro=50000000&sueldo=1200000",
					"protocol": "http",
					"host": [
						"localhost"
					],
					"port": "8080",
					"path": [
						"rest",
						"msdxc",
						"dxc"
					],
					"query": [
						{
							"key": "ahorro",
